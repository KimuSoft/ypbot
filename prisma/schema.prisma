datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id String @id

  discordAccessToken    String   @ignore
  discordRefreshToken   String
  discordTokenExpiresAt DateTime

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
